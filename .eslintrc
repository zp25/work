{
  "root": true,
  "env": {
    "browser": true,
    "jquery": true,
    "es6": true
  },
  "globals": {
    "DEBUG": true,
    "$": false,
    "Base": false,
    "Console": false
  },
  "rules": {
    "no-console": 0,
    "no-cond-assign": ["error", "except-parens"],
    "no-param-reassign": [
      "error", { "props": false }
    ]
  },
  "extends": "airbnb-base"
}
